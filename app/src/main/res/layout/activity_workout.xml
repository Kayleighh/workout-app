        android:textSize="12sp"
        android:fontFamily="@font/montserrat"
        android:textColor="#FFFFFF"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="@+id/textView36"
        app:layout_constraintStart_toStartOf="@+id/textView36"
