        app:layout_constraintEnd_toEndOf="@+id/textView38"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <Button
        android:id="@+id/btnFinishWorkout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/montserrat"
        android:text="FINISH"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/holderWorkout"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/holderWorkout"
        app:layout_constraintTop_toBottomOf="@+id/holderWorkout" />

</androidx.constraintlayout.widget.ConstraintLayout>